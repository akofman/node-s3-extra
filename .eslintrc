{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended", "prettier"],
  "plugins": ["prettier", "jest"],
  "rules": {
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "no-continue": "off",
    "no-lonely-if": "off",
    "no-param-reassign": "off",
    "consistent-return": "off",
    "prefer-spread": "off",
    "no-underscore-dangle": "off"
  }
}